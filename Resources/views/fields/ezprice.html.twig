{% extends "EzPublishCoreBundle::content_fields.html.twig" %}

{% block ezprice_field %}
{% spaceless %}
    <pre>{{ dump( field ) }}</pre>
    {% set price = ezprice_value( versionInfo, field ) %}
    Price including VAT: {{ price.priceIncludingVat }}<br/>
    Price excluding VAT: {{ price.priceExcludingVat }}
{% endspaceless %}
{% endblock %}
